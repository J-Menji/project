   <children>
      <Button fx:id="appointmentsButton" layoutX="138.0" layoutY="70.0" mnemonicParsing="false" onAction="#appointmentsButtonClicked" prefHeight="45.0" prefWidth="135.0" text="Appointments" />
      <Button fx:id="customerButton" layoutX="140.0" layoutY="148.0" mnemonicParsing="false" onAction="#customerButtonClicked" prefHeight="45.0" prefWidth="135.0" text="Customers" />
      <Button fx:id="reportsButton" layoutX="140.0" layoutY="225.0" mnemonicParsing="false" onAction="#reportsButtonCLicked" prefHeight="45.0" prefWidth="135.0" text="Reports" />
      <Button layoutX="308.0" layoutY="370.0" mnemonicParsing="false" text="Exit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
